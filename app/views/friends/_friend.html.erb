<tr id="<%= dom_id friend %>">
  <td><%= friend.first_name %></td>
  <td><%= friend.last_name %></td>
  <td><%= friend.email %></td>
  <td><%= friend.phone %></td>
  <td><%= friend.twitter %></td>
  <td class="d-flex justify-content-between">
    <%= link_to "View", friend, class:"btn btn-secondary" %>
    <%= link_to "Edit", edit_friend_path(friend), class:"btn btn-primary" %>
    <%= button_to "Delete", friend, method: :delete, class:"btn btn-danger" %>
  </td>
</tr>
